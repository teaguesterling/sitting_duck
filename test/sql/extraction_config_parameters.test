# Test 5: parse_ast function with extraction config
query IIII
SELECT name, start_line, parent_id, peek
FROM parse_ast('def hello(): pass', 'python', 
    context := 'normalized', 
    source := 'lines', 
    structure := 'minimal', 
    peek := 'smart')
LIMIT 3;
----
(empty)	1	NULL	def hello(): pass
hello	1	0	def hello(): pass
(empty)	1	1	def

