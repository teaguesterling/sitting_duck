# Test 8: Session state isolation
# ===============================
query I
SELECT 
    (exec1.count = exec2.count) as results_consistent
FROM (
    SELECT COUNT(*) as count
    FROM read_ast('test/data/python/simple.py', 'python', context := 'native')
) exec1,
(
    SELECT COUNT(*) as count
    FROM read_ast('test/data/python/simple.py', 'python', context := 'native')
) exec2;
----
true

